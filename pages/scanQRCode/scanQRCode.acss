/**index.wxss**/
page{
  height: 100%;
  overflow: hidden;
  background-image: url('data:image/png;base64,');
  background-size: 108rpx 104rpx;
}
.img{
  width: 682rpx;
  height:682rpx;
  position: fixed;
  top: 50%;
  left: 50%;
  margin-top: -480rpx;
  margin-left: -341rpx;
  z-index: 0
}
.scan{
  height: 260rpx;
  width: 750rpx;
  text-align: center;
  position: fixed;
  bottom: 100rpx;
  z-index: 1;
}

.scan .scan-code{
	width: 427rpx;
	height: 94rpx;
  line-height: 94rpx;
  color: #fff;
	background: #95b778 ;
	border-radius: 47rpx;
  margin: 0 auto 45rpx;
  text-align: left;
  font-size: 40rpx;
}
.scan .scan-code text{
  font-size: 40rpx;
  margin-left: -10rpx;
}
.scan .scan-code .icon{
  float: left;
  margin-left: 115rpx;
  margin-right: 12rpx;
}
.scan .slogan {
  line-height: 1.4em;
  font-size: 40rpx;
  font-weight: normal;
  font-stretch: normal;
  font-family: AdobeHeitiStd-Regular;
  color: #95b778;
}
.scan .slogan text{
  display: block;
  font-size: 30rpx;
}
.toFeedback{
  position: fixed;
  right: 30rpx;
